# Database
DATABASE_URL=postgresql://user:password@localhost:5432/estimation_db
REDIS_URL=redis://localhost:6379
REPORTS_TABLE_NAME=estimation-reports
S3_REPORT_BUCKET=estimation-reports-bucket

# API Settings
SECRET_KEY=your-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Frontend
VITE_API_URL=/
VITE_EXCEL_API_ENABLED=true
VITE_DISABLE_AUTH=false
VITE_BACKEND_TARGET=legacy
VITE_APP_ENV=dev
VITE_COGNITO_REGION=us-east-1
VITE_COGNITO_CLIENT_ID=your-cognito-app-client-id

# Auth
AUTH_REQUIRED=true
COGNITO_REGION=us-east-1
COGNITO_USER_POOL_ID=your-cognito-user-pool-id
COGNITO_CLIENT_ID=your-cognito-app-client-id

# Excel Integration (Optional)
MICROSOFT_CLIENT_ID=your-client-id
MICROSOFT_CLIENT_SECRET=your-client-secret

# AI (Optional)
# Used by backend/app/services/ai_service.py
OPENAI_API_KEY=your-openai-api-key

# SAM.gov Contracts (Optional)
SAM_API_KEY=your-sam-api-key
SAM_SYNC_SCHEDULED=true
SAM_SYNC_INTERVAL_MINUTES=1440
SAM_SYNC_MIN_INTERVAL_MINUTES=1440
SAM_SYNC_MAX_REQUESTS_PER_DAY=10
SAM_SYNC_DAYS_BACK=7
SAM_SYNC_QUERY=
SAM_SYNC_LIMIT=1000
SAM_SYNC_PAGES=1
